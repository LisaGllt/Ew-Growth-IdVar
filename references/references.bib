}

@article{spurgeon_2000,
  title = {Relative Sensitivity of Life-Cycle and Biomarker Responses in Four Earthworm Species Exposed to Zinc},
  author = {Spurgeon, David J. and Svendsen, Claus and Rimmer, Viv R. and Hopkin, Stephen P. and Weeks, Jason M.},
