  author = {Thuiller, Wilfried and Münkemüller, Tamara and Lavergne, Sébastien and Mouillot, David and Mouquet, Nicolas and Schiffers, Katja and Gravel, Dominique},
  date = {2013},
  journaltitle = {Ecology Letters},
  volume = {16},
