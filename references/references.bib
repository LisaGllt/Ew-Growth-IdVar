@article{pantelakiOccurrenceRemovalOrganophosphate2022,
  title = {Occurrence and Removal of Organophosphate Esters in Municipal Wastewater Treatment Plants in {{Thessaloniki}}, {{Greece}}},
  author = {Pantelaki, Ioanna and Voutsa, Dimitra},
  date = {2022-11},
