@article{vogwill_2012a,
  title = {The Experimental Evolution of Herbicide Resistance in {{{\emph{Chlamydomonas}}}}{\emph{ Reinhardtii}} Results in a Positive Correlation between Fitness in the Presence and Absence of Herbicides},
