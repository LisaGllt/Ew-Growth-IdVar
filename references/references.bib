  author = {Sanderson, Sarah and Bolnick, Daniel I. and Kinnison, Michael T. and O'Dea, Rose E. and Gorné, Lucas D. and Hendry, Andrew P. and Gotanda, Kiyoko M.},
  date = {2023},
  journaltitle = {Ecology Letters},
