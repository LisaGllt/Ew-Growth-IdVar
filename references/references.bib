  author = {Pelosi, C. and Bertrand, C. and Daniele, G. and Coeurdassier, M. and Benoit, P. and Nélieu, S. and Lafay, F. and Bretagnolle, V. and Gaba, S. and Vulliet, E. and Fritsch, C.},
