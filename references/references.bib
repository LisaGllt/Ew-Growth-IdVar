  shorttitle = {Small Streams–Large Concentrations?},
  author = {Halbach, Katharina and Möder, Monika and Schrader, Steffi and Liebmann, Liana and Schäfer, Ralf B. and Schneeweiss, Anke and Schreiner, Verena C. and Vormeier, Philipp and Weisner, Oliver and Liess, Matthias and Reemtsma, Thorsten},
  date = {2021-09},
  journaltitle = {Water Research},
