@article{silva_2019,
  title = {Pesticide Residues in {{European}} Agricultural Soils – {{A}} Hidden Reality Unfolded},
  author = {Silva, Vera and Mol, Hans G.J. and Zomer, Paul and Tienstra, Marc and Ritsema, Coen J. and Geissen, Violette},
  date = {2019-02},
