  url = {https://onlinelibrary.wiley.com/doi/abs/10.2903/j.efsa.2015.3996},
  urldate = {2022-11-25},
  abstract = {Following a request from the European Food Safety Authority, the Panel on Plant Protection Products and their Residues developed an opinion on the science to support the development of a risk assessment scheme of plant protection products for non-target arthropods. The current risk assessment scheme is reviewed, taking into consideration recent workshops and progress in science. Proposals are made for specific protection goals which aim to protect important ecosystem services such as food web support, pest control and biodiversity. In order to address recovery and source–sink population dynamics, conducting a landscape-level risk assessment is suggested. A new risk assessment scheme is suggested which integrates modelling approaches. The main exposure routes for non-target arthropods are identified and proposals are made on how to integrate them in the risk assessment. The appropriateness of the currently used vegetation distribution factor was investigated. It is proposed that new tests be included in order to address exposure via oral uptake of residues and uncertainties related to differences in species sensitivity.},
  langid = {english},
  keywords = {effects,exposure,insects,non-target arthropods,notion,pesticides,protection goals,risk assessment},
  file = {/Users/lisagollot/Zotero/storage/3QD8KT7I/EFSA Panel on Plant Protection Products and their Residues (PPR) - 2015 - Scientific Opinion addressing the state of the sci.pdf;/Users/lisagollot/Zotero/storage/K8QJ5PM2/j.efsa.2015.html}
}

